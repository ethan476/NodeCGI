{
	"virtualHosts": [

		{
			"domain": "localhost",
			"documentRoot": "./docroot",
			"specialFiles": {
				"__404__": "404.lisp",
				"__500__": "",
				"__directory__": ""
			},
			"ssl": {
				"useSSL": false,
				"privateKey": "",
				"certificate": ""
			},
			"port": 8081
		}

	],

	"virtualHostPartialMatching": false,

	"extensions": {
		".lisp": {
			"handler": "clisp",
			"default-content-type": "text/html"
		},
		".php": {
			"handler": "php",
			"iniPath": "/etc/",
			"default-content-type": "text/html"
		}
	},

	"directoryListing": true,

	"timeout": 15000,

	"indexFiles": [
		"index.lisp"
	],

	"handlerPaths": [
		"./handlers"
	],

	"cgi": {
		"GATEWAY_INTERFACE": "CGI/1.1",
		"SERVER_ADMIN":	"admin@example.org",
		"SERVER_NAME": "example.org",
		"SERVER_SOFTWARE": "NodeGCI/1.1"
	},

	"env": {

	},

